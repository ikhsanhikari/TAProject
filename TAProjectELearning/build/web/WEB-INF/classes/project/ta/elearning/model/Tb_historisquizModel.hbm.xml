<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_historisquizModel" table="tb_historisquiz">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="id_colleger" column="id_colleger" type="integer" />
        <property name="id_matery" column="id_matery" type="integer" /> 
        <property name="id_qa" column="id_qa" type="integer" />
        <property name="status" column="status" type="integer" />
        <property name="timeupdate" column="timeupdate" type="string"/>
    </class>
  

</hibernate-mapping>
