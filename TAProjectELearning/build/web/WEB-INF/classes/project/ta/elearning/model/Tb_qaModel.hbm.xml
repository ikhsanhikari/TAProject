<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_qaModel" table="tb_qa">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="id_quiz" column="id_quiz" type="string" />
        <property name="id_answers" column="id_answers" type="string" />
        <property name="id_status" column="id_status" type="string" />
        <property name="id_level" column="id_level" type="string" />
        <property name="id_jenis_soal" column="id_jenis_soal" type="string" />
        <property name="timeopen" column="timeopen" type="string" />
        <property name="timeclose" column="timeclose" type="string" />
        <property name="timecreated" column="timecreated" type="string" />
        <property name="timemodified" column="timemodified" type="string" />
        

    </class>
  

</hibernate-mapping>
