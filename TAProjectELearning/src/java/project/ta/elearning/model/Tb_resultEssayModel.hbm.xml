<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_resultEssayModel" table="tb_resultessay">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="id_user" column="id_user" type="integer"/>
        <property name="id_essay" column="id_essay" type="integer"/>
        <property name="jawaban" column="jawaban" type="string"/>
        <property name="nilai" column="nilai" type="integer"/>
        <property name="latihan_ke" column="latihan_ke" type="string"/>
        <property name="id_korektor" column="id_korektor" type="integer"/>
        <property name="timeopen" column="timeopen" type="string"/>
        <property name="timeupdate" column="timeupdate" type="string"/>
        <property name="pathName" column="pathName" type="string"/>
    </class>
        
  

</hibernate-mapping>
