<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="project.ta.elearning.model.Tb_modelModel" table="tb_model">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="id_user"  type="integer"/>
        <property name="id_matery"  type="integer"/>
        <property name="knowledge_sebelum"  type="integer"/>
        <property name="knowledge_sesudah"  type="integer"/>
        <property name="score_sebelum"  type="integer"/>
        <property name="score_sesudah"  type="integer"/>
        
    </class>
  

</hibernate-mapping>
